{
  "expo": {
    "name": "Quilt",
    "slug": "Quilt",
    "scheme": "expo-supabase-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "cover",
          "image": "./assets/splash-dark.png"
        }
      },
      "bundleIdentifier": "com.anonymous.Quilt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "cover",
          "image": "./assets/splash-dark.png"
        }
      },
      "package": "com.anonymous.Quilt"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "fonts": [
      {
        "asset": "./assets/fonts/proximanova_regular.otf",
        "family": "ProximaNova-Regular"
      },
      {
        "asset": "./assets/fonts/proximanova_bold.otf",
        "family": "ProximaNova-Bold"
      },
      {
        "asset": "./assets/fonts/proximanova_light.otf",
        "family": "ProximaNova-Light"
      },
      {
        "asset": "./assets/fonts/proximanova_medium.otf",
        "family": "ProximaNova-Medium"
      },
      {
        "asset": "./assets/fonts/proximanova_semibold.otf",
        "family": "ProximaNova-Semibold"
      }
    ]
  }
}
